{
    "name": "filter-component-oih",
    "version": "2.2.1",
    "description": "Open Integration Hub filter component to filter incoming data based on arbitrary expression. Based on elastic.io filter component version.",
    "homepage": "https://www.openintegrationhub.com",
    "scripts": {
        "start": "node ./node_modules/@openintegrationhub/ferryman/runGlobal.js",
        "pretest": "eslint lib spec --fix",
        "test": "mocha spec --recursive"
    },
    "engines": {
        "node": "16"
    },
    "dependencies": {
        "@blendededge/ferryman-extensions": "^2.0.1",
        "@openintegrationhub/ferryman": "^2.3.0"
    },
    "devDependencies": {
        "bunyan": "1.8.12",
        "chai": "^4.3.8",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.50.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-mocha": "^10.2.0",
        "grunt-mocha-test": "0.13.3",
        "mocha": "^10.2.0",
        "nock": "^13.3.3",
        "sinon": "^16.0.0"
    },
    "eslintConfig": {
        "extends": "xo-space",
        "env": {
            "mocha": true
        }
    },
    "keywords": [
        "openintegrationhub",
        "filter",
        "ipaas",
        "integration",
        "component"
    ],
    "author": {
        "name": "Blended Edge LLC",
        "email": "info@blendededge.com",
        "url": "https://blendededge.com"
    },
