{
    "name": "filter-component-oih",
    "version": "2.0.0",
    "description": "Open Integration Hub filter component to filter incoming data based on arbitrary expression. Based on elastic.io filter component version.",
    "homepage": "https://www.openintegrationhub.com",
    "scripts": {
        "start": "node ./node_modules/@openintegrationhub/ferryman/runGlobal.js",
        "pretest": "eslint lib spec --fix",
        "test": "mocha spec --recursive"
    },
    "engines": {
        "node": "14"
    },
    "dependencies": {
        "@blendededge/ferryman-extensions": "^1.0.4",
        "@openintegrationhub/ferryman": "1.5.7",
        "moment": "2.29.2",
        "request": "2.88.2",
        "request-promise": "4.2.4"
    },
    "devDependencies": {
        "bunyan": "1.8.12",
        "chai": "3.5.0",
        "chai-as-promised": "7.1.1",
        "eslint": "5.16.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-plugin-import": "2.18.0",
        "eslint-plugin-mocha": "4.8.0",
        "grunt-mocha-test": "0.13.3",
        "mocha": "8.2.1",
        "nock": "9.1.6",
        "sinon": "7.5.0"
    },
    "eslintConfig": {
        "extends": "xo-space",
        "env": {
            "mocha": true
        }
    },
    "keywords": [
        "openintegrationhub",
        "filter",
        "ipaas",
        "integration",
        "component"
    ],
    "author": {
        "name": "Blended Edge LLC",
        "email": "info@blendededge.com",
        "url": "https://blendededge.com"
    },
    "repository": "blendededge/filter-component",
    "license": "Apache-2.0"
}
